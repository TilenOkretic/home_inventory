{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "User",
    "description": "The user.",
    "default": {},
    "required": [
        "email",
        "name",
        "password"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/id",
            "type": "integer",
            "title": "The ID",
            "description": "An explanation about the purpose of this instance."
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The user's email. Must be unique!",
            "examples": [
                "tilen.okretic@gmail.com"
            ]
        },
        "name": {
            "$id": "#/properties/name",
            "type": "string",
            "title": "The user's name.",
            "examples": [
                "Tilen"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The user's hashed password."
        },
        "created_at": {
            "$id": "#/properties/created_at",
            "type": "string",
            "title": "The creation date of user!"
        },
        "updated_at": {
            "$id": "#/properties/updated_at",
            "type": "string",
            "title": "The date user has been last updated!"
        }
    },
    "additionalProperties": false
}