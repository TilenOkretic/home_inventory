{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "title": "Item Info",
    "description": "The item info.",
    "default": {},
    "required": [
        "user_id",
        "item_id",
        "purchase-date",
        "inventory_location_id"
    ],
    "properties": {
        "id": {
            "$id": "#/properties/item_id",
            "type": "integer",
            "title": "The item ID",
            "description": "An explanation about the purpose of this instance."
        },
        "purchase_date": {
            "$id": "#/properties/purchase_date",
            "type": "string",
            "format": "date",
            "title": "The item ID",
            "description": "Date of purchase"
        },
        "experation_date": {
            "$id": "#/properties/experation_date",
            "type": "string",
            "format": "date",
            "title": "The item ID",
            "description": "Date of experation"
        },
        "item_id": {
            "$id": "#/properties/item_id",
            "type": "integer",
            "title": "The ID",
            "description": "An explanation about the purpose of this instance."
        },
        "retailer_id": {
            "$id": "#/properties/retailer_id",
            "type": "integer",
            "title": "The ID",
            "description": "ID of the reatailer."
        },
        "last_used": {
            "$id": "#/properties/experation_date",
            "type": "string",
            "format": "date",
            "title": "last use",
            "description": "Date of last use."
        },
        "item_type_id": {
            "$id": "#/properties/item_type_id",
            "type": "number",
            "title": "The id of the item type"
        },
        "purchase_price": {
            "$id": "#/properties/purchase_price",
            "type": "number",
            "minimum": 0,
            "title": "The item purchase_price",
            "description": "Item purchase_price"
        },
        "msrp": {
            "$id": "#/properties/msrp",
            "type": "number",
            "minimum": 0,
            "title": "The item msrp",
            "description": "Item msrp"
        },
        "inventory_location_id": {
            "$id": "#/properties/inventory_location_id",
            "type": "number",
            "title": "The inventory location id."
        },
        "user_id": {
            "$id": "#/properties/user_id",
            "type": "number",
            "title": "The id of the user."
        }
    },
    "additionalProperties": false
}